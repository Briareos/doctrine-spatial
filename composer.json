{
    "name": "yvann/doctrine-spatial-bundle",
    "type": "symfony-bundle",
    "description": "Provides a doctrine spatial integration for your Symfony2 Project.",
    "keywords": ["doctrine spatial"],
    "authors": [
        {"name": "Yvann Boucher", "email": "yvann.boucher@gmail.com"}
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/console": "master-dev",
        "symfony/class-loader": "master-dev",
        "doctrine/common": "master-dev",
        "doctrine/dbal": "master-dev",
        "doctrine/orm": "master-dev",
    },
    "autoload": {
        "psr-0": { "Ybo\\DoctrineSpatialBundle": "" }
    },
    "target-dir": "Ybo/DoctrineSpatialBundle"
}
